-// just re-exporting the interface that we want.
+    #[cfg(not(all(doc, semver_rustdoc_workaround)))]
+    pub const EMPTY: Self = BuildMetadata {
+        identifier: Identifier::empty(),
+    };
 
-pub use version::{Version, Identifier, SemVerError};
